rotate <- function(m){
new_m <- m
for (i in 1:nrow(m)) {
new_m[,nrow(m)-i+1] <- m[i,]
}
return(new_m)
}
m <- matrix(1:8, nrow=2, ncol=4)
m
m <- rotate(m)
